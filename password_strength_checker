import string
print("Enter your password")
password = str(input("My Password is:"))

if len(password) >= 12 and any(char.isupper() for char in password) \
    and any(char.islower() for char in password) \
    and any(string.punctuation for char in password):
    print("Your password strength is Very strong!!")
else:
    print("Your password is weak.")
            